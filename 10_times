#!/bin/bash
# SPDX-FileCopyrightText: 2024 Ben
# SPDX-License-Identifier: BSD-3-Clause

while read -r line; do
    # 入力の空白や改行を削除
    input=$(echo "$line" | xargs)

    # 数値かどうかをチェック
    if [[ "$input" =~ ^[0-9]+$ ]]; then
        # 数値を10倍にして表示
        result=$((input * 10))
        echo "$result"
    else
        # 数値以外の入力があった場合
        echo "正確な数値ではありません。"
    fi
done
